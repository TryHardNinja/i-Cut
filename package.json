  "description": "Personal technical style and development direction",
  "main": "gulpfile.js",
  "scripts": {
    "dev": "npm run gulp && npm run clhtml && npm run rollup && npm run rev",
    "gulp": "gulp distDelFile && gulp svgDeal && gulp allmin && gulp webp && gulp Base64",
    "rev": "gulp revDelfile && gulp revDeal && gulp revHtml && gulp htmlUrl && gulp htmlMin",
    "clhtml": "node ./build/critical.js",
    "rollup": "rollup -c ./build/rollup.config.js",
    "rmin": "npm run rpts && npm run rpbl",
    "rpts": "rollup -c ./build/rollup.config.ts.js",
    "rpbl": "rollup -c ./build/rollup.config.es5.js",
    "v-dist": "webpack --progress --config ./build/webpack.config.build.js",
    "v-src": "webpack-dev-server --inline --hot --config ./build/webpack.config.js",
    "karma": "karma start ./build/karma.conf.js --single-run",
    "sauce": "karma start ./build/karma.sauce.js --single-run"
  },
