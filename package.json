{
  "name": "i-cut",
  "version": "2.6.2",
  "description": "Personal technical style and development direction",
  "main": "gulpfile.js",
  "scripts": {
    "gulp": "gulp default --color && npm run clhtml && npm run rollup && npm run rev",
    "clhtml": "node ./build/critical.js",
    "rollup": "rollup -c ./build/rollup.config.js",
    "rev": "gulp revAll --color && gulp htmlAll --color",
    "rmin": "npm run rpts && npm run rpbl",
    "rpts": "rollup -c ./build/rollup.config.ts.js",
    "rpbl": "rollup -c ./build/rollup.config.es5.js",
    "build": "node ./build/build.js",
    "dev": "webpack-dev-server --inline --progress --config ./build/webpack.config.dev.js",
    "test": "karma start ./build/karma.conf.js --single-run",
    "sauce": "karma start ./build/karma.sauce.js --single-run",
    "ssr": "rimraf v-dist && npm run build:client && npm run build:server && npm run build:node",  
    "build:node":"cross-env NODE_ENV=production node server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.server.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhenbinjing/i-Cut.git"
  },
  "keywords": [
    "rem",
    "adaptive",
    "responsive",
    "flex",
    "flexbox",
    "flexgrid",
    "grid",
    "gulp",
    "rollup",
    "webpack"
  ],
  "author": "zhenbinjing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhenbinjing/i-Cut/issues"
  },
