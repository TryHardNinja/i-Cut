{{#sprites}}
.icon-sprite { 
	display: inline-block;
	background: url("../img/icon/sprite.png");
	background-repeat: no-repeat;
	background-size: {{px.total_width}} {{px.total_height}};
}
{{/sprites}}
{{#sprites}}
.{{name}}{
	background-position: {{px.offset_x}} {{px.offset_y}};
    width: {{px.width}};
	height: {{px.height}};	
}
{{/sprites}}

